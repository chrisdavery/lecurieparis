    "image_with_text_t7WmFr": {
      "type": "image-with-text",
      "blocks": {
        "heading_7e3mkE": {
          "type": "heading",
          "settings": {
            "title": "Only the Finest Materials",
            "heading_tag": "h2",
            "heading_font_class": "fs-heading-1-base"
