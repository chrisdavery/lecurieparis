        },
        "block-2": {
          "type": "text",
          "settings": {
            "text": "<p>• French design<br\/>• Ultra-lightweight titanium<br\/>• Finished in 18K gold or 14K white gold<br\/>• Engraved LP nose pads<br\/>• Zeiss sun lenses with interior anti-reflection treatment<br\/>• Premium cellulose acetate<br\/>• Made in Japan: Papillon, Morgane, Panam, Leo, Paul<br\/>• Made in Italy: Barbara<\/p>",
            "text_font_class": "fs-body-75"
          }
        },
        "block-3": {
          "type": "button",
          "settings": {
            "link": "shopify:\/\/collections\/luxury-eyewear",
