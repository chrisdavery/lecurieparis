{% style %}
@font-face {
    font-family: 'Aparajita';
    src: url({{ 'Aparajita-Italic.woff2' | asset_url }}) format('woff2'),
        url({{ 'Aparajita-Italic.woff' | asset_url }}) format('woff');
    font-weight: normal;
    font-style: italic;
    font-display: swap;
}
@font-face {
    font-family: 'Aparajita';
    src: url({{ 'Aparajita-BoldItalic.woff2' | asset_url }}) format('woff2'),
        url({{ 'Aparajita-BoldItalic.woff' | asset_url }}) format('woff');
    font-weight: bold;
    font-style: italic;
    font-display: swap;
}

@font-face {
    font-family: 'Aparajita';
    src: url({{ 'Aparajita.woff2' | asset_url }}) format('woff2'),
        url({{ 'Aparajita.woff' | asset_url }}) format('woff');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}



@font-face {
    font-family: 'Times New Roman';
    src: url({{ 'TimesNewRomanPSMT.woff2' | asset_url }}) format('woff2'),
        url({{ 'TimesNewRomanPSMT.woff' | asset_url }}) format('woff');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}
@font-face {
    font-family: 'Helvetica Neue';
    src: url({{ 'HelveticaNeue-Bold.woff2' | asset_url }}) format('woff2'),
        url({{ 'HelveticaNeue-Bold.woff' | asset_url }}) format('woff');
    font-weight: bold;
    font-style: normal;
    font-display: swap;
}
{% endstyle %}