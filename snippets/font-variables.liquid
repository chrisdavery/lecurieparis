{% style %}
@font-face {
    font-family: 'Aparajita';
    src: url({{ 'Aparajita-Italic.woff2' | asset_url }}) format('woff2'),
        url({{ 'Aparajita-Italic.woff' | asset_url }}) format('woff');
    font-weight: normal;
    font-style: italic;
    font-display: swap;
}
@font-face {
    font-family: 'Times New Roman';
    src: url({{ 'TimesNewRomanPSMT.woff2' | asset_url }}) format('woff2'),
        url({{ 'TimesNewRomanPSMT.woff' | asset_url }}) format('woff');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}
@font-face {
    font-family: 'Helvetica Neue LT Pro';
    src: url({{ 'HelveticaNeueLTPro-BdCn.woff2' | asset_url }}) format('woff2'),
        url({{ 'HelveticaNeueLTPro-BdCn.woff' | asset_url }}) format('woff');
    font-weight: bold;
    font-style: normal;
    font-display: swap;
}
{% endstyle %}